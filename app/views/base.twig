<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{ title }}</title>
  <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
  <link rel="icon" href="{{ urlFor('root') }}public/img/pokebola.gif" type="image/x-icon">
  <!-- PACE js -->
  <script data-pace-options='{ "ajax": false, "restartOnRequestAfter": false }' src="{{ urlFor('root') }}public/js/pace.min.js"></script>
  <!-- CSS -->
  <link rel="stylesheet" href="{{ urlFor('root') }}public/css/bootstrap.css">
  <link rel="stylesheet" href="{{ urlFor('root') }}public/css/font-awesome.css">
  <link rel="stylesheet" href="{{ urlFor('root') }}public/css/animate.min.css">
  <link rel="stylesheet" href="{{ urlFor('root') }}public/css/styles.css">
  {% block css %}{% endblock %}
</head>
<body class="container">
  <!--[if lt IE 8]> -->
  <div class="row">
    <div class="col-md-12">
      {% spaceless %}
      {% if user is defined %}
          {% include 'navbar.twig' %}
      {% endif %}
      {% endspaceless %}
    </div>
  </div>
  <hr>
  {% block content %}{% endblock %}
  <!-- JS -->
  <script src="{{ urlFor('root') }}public/js/jquery.js"></script>
  <script src="{{ urlFor('root') }}public/js/bootstrap.min.js"></script>
  {% block js %}{% endblock %}
</body>
</html>
